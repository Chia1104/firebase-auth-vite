    "child-process-promise": "^2.2.1",
    "exif-reader": "^1.1.0",
    "exifr": "^7.1.3",
    "exifreader": "^4.9.1",
    "firebase-admin": "^11.2.0",
    "firebase-functions": "^4.0.2",
    "gm": "^1.23.1",
    "mkdirp": "^2.1.3",
    "pluscodes": "^2.6.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
